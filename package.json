{
  "name": "json-accessor",
  "version": "1.0.0",
  "description": "Production-grade path-based JSON operations engine for safe CRUD operations on deeply nested structures",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test",
    "benchmark": "node benchmarks/run.js"
  },
  "keywords": [
    "json",
    "path",
    "nested",
    "crud",
    "immutable",
    "deep",
    "lodash",
    "object",
    "manipulation",
    "flatten",
    "diff",
    "patch",
    "audit",
    "typescript"
  ],
  "author": "Rahul Kumar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RahulsKumaroks/json-accessor.git"
  },
  "bugs": {
    "url": "https://github.com/RahulsKumaroks/json-accessor/issues"
  },
  "homepage": "https://github.com/RahulsKumaroks/json-accessor#readme",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}